async function getSKEntities(html, taxonomyType) {
var url = "https://nlp.sportskeeda.com/analyzeEntities";
var payload = {
"text": html
}
if (taxonomyType === "wiki" || taxonomyType === "wiki_tag") {
var payload = {
"text": html,
