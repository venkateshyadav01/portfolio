<script>
var listeners = (function() {
function enableTooltips(quillDiv) {
let tooltips = {
'table' : 'Insert Table',
'header' : 'Headings',
'bold': 'Bold',
'italic': 'Italic',
'underline': 'Underline',
'strike': 'Strike',
